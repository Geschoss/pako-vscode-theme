{
    "name": "pako-vscode-theme",
    "displayName": "pako-vscode-theme",
    "description": "theme for vscode",
    "publisher": "pako",
    "version": "0.21.0",
    "engines": {
        "vscode": "^1.63.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Geschoss/pako-vscode-theme"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "pascal",
                "aliases": [
                    "Pascal",
                    "pascal"
                ],
                "extensions": [
                    ".pas",
                    ".p",
                    ".dfm",
                    ".fmx",
                    ".dpr",
                    ".dpk",
                    ".lfm",
                    ".dpr",
                    ".lpr"
                ],
                "configuration": "./language/pascal/pascal.configuration.json"
            },
            {
                "id": "sql",
                "extensions": [
                    ".sql"
                ]
            }
        ],
        "grammars": [
            {
                "language": "pascal",
                "scopeName": "source.pascal",
                "path": "./language/pascal/pascal.tmLanguage"
            },
            {
                "language": "sql",
                "scopeName": "source.sql",
                "path": "./language/sql/SQL.plist"
            }
        ],
        "themes": [
            {
                "id": "pako-white",
                "label": "pako-white",
                "uiTheme": "vs",
                "path": "./theme/white-color-theme.json"
            },
            {
                "id": "pako-dark",
                "label": "pako-dark",
                "uiTheme": "vs-dark",
                "path": "./theme/dark-color-theme.json"
            }
        ]
    },
    "license": "(MIT OR Apache-2.0)"
}
