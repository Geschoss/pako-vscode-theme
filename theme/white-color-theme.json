{
    "$schema": "vscode://schemas/color-theme",
    "name": "pako-white",
    "tokenColors": [
        {
            "scope": ["keyword.other.fn.rust"],
            "settings": {
                "foreground": "#5186A7"
            }
        },
        {
            "scope": ["entity.name.function.macro.rust"],
            "settings": {
                "foreground": "#7A3E9D"
            }
        },
        {
            "scope": ["heading.3.markdown"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#268BD2"
            }
        },
        {
            "scope": ["meta.template.expression.js"],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#d38b86"
            }
        },
        {
            "name": "Strings",
            "scope": ["string", "string.regexp", "constant.other.symbol"],
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "Strings: Escape Sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#777777"
            }
        },
        {
            "name": "Numbers, Characters",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant.keyword",
                "constant"
            ],
            "settings": {
                "foreground": "#7A3E9D"
            }
        },
        {
            "name": "Global definitions",
            "scope": "entity.name",
            "settings": {
                "foreground": "#325CC0"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#777777"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "background": "#96000014",
                "foreground": "#660000"
            }
        },
        {
            "name": "Extra: Diff Range",
            "scope": ["meta.diff.range", "meta.diff.index", "meta.separator"],
            "settings": {
                "background": "#DDDDFF",
                "foreground": "#434343"
            }
        },
        {
            "name": "Extra: Diff From",
            "scope": "meta.diff.header.from-file",
            "settings": {
                "background": "#FFDDDD",
                "foreground": "#434343"
            }
        },
        {
            "name": "Extra: Diff To",
            "scope": "meta.diff.header.to-file",
            "settings": {
                "background": "#DDFFDD",
                "foreground": "#434343"
            }
        },
        {
            "scope": [
                "meta.type",
                "keyword.control",
                "entity.name.type",
                "keyword.operator.new",
                "storage.modifier.async.js",
                "keyword.operator.expression.delete.js"
            ],
            "settings": {
                "foreground": "#0d4a91"
            }
        },
        {
            "scope": ["storage.modifier.js", "storage.type"],
            "settings": {
                "foreground": "#5186a7"
            }
        },
        {
            "scope": ["variable.language.this.ts"],
            "settings": {
                "foreground": "#5186a7"
            }
        },
        {
            "scope": "entity.name",
            "settings": {
                "foreground": "#2B343F"
            }
        },
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#d38b86"
            }
        },
        {
            "name": "SQL_3",
            "scope": [
                "keyword.pascal",
                "keyword.other.DML.sql",
                "keyword.other.sql",
                "meta.alter.sql",
                "storage.modifier.sql",
                "keyword.other.create.sql",
                "keyword.other.order.sql",
                "keyword.other.DDL.create.II.sql"
            ],
            "settings": {
                "foreground": "#325CC0"
            }
        },
        {
            "scope": [
                "variable.language.this.js",
                "storage.support.type.pascal",
                "support.type.primitive.ts",
                "support.type.primitive.tsx"
            ],
            "settings": {
                "foreground": "#CC8BC9"
            }
        },

        {
            "scope": ["markup.heading.markdown"],
            "settings": {
                "foreground": "#268BD2",
                "fontStyle": "bold"
            }
        },
        {
            "scope": ["markup.italic.markdown"],
            "settings": {
                "foreground": "#5186a7",
                "fontStyle": "italic"
            }
        },
        {
            "scope": ["markup.bold.markdown"],
            "settings": {
                "foreground": "#5186a7",
                "fontStyle": "bold"
            }
        }
    ],
    "colors": {
        "editor.lineHighlightBackground": "#bfdbfe1f",
        "list.activeSelectionBackground": "#cce5ff",
        "list.activeSelectionForeground": "#000000",
        "list.focusOutline": "#1582f7",
        "list.focusForeground": "#000000",
        "list.hoverBackground": "#cce5ff63",
        "list.hoverForeground": "#000000",
        "list.inactiveSelectionBackground": "#cce5ff80",
        "list.inactiveSelectionForeground": "#000000",
        "editor.background": "#F7F7F7",
        "editor.foreground": "#000",
        "editor.selectionBackground": "#BFDBFE",
        "editor.selectionHighlightBackground": "#CFCFCF81",
        "panel.background": "#F0F0F0",
        "sideBar.background": "#F0F0F0",
        "editorGroupHeader.tabsBackground": "#F0F0F0",
        "activityBar.background": "#F0F0F0",
        "activityBar.foreground": "#007ACC",
        "editorLineNumber.foreground": "#9DA39A",
        "editorCursor.foreground": "#007ACC",
        "editor.findMatchBackground": "#FFBC5D",
        "editor.findMatchHighlightBackground": "#FFD86381",
        "statusBar.background": "#DDDDDD",
        "statusBar.foreground": "#474747",
        "statusBar.debuggingBackground": "#FFBC5D",
        "statusBar.debuggingForeground": "#000",
        "statusBar.noFolderBackground": "#7A3E9D",
        "statusBar.noFolderForeground": "#fff",
        "list.focusHighlightForeground": "#007ACC",
        "focusBorder": "#CCCCCC",
        "quickInputList.focusForeground": "#000000",
        "quickInputList.focusBackground": "#DDDDDD",
        "editorSuggestWidget.selectedBackground": "#DDDDDD",
        "editorSuggestWidget.focusHighlightForeground": "#000000",
        "editorSuggestWidget.highlightForeground": "#000000",
        "editorSuggestWidget.selectedForeground": "#000000",
        "terminal.ansiWhite": "#BBBBBB",
        "terminal.ansiBlack": "#000000",
        "terminal.ansiBlue": "#325CC0",
        "terminal.ansiCyan": "#0083B2",
        "terminal.ansiGreen": "#448C27",
        "terminal.ansiMagenta": "#7A3E9D",
        "terminal.ansiRed": "#AA3731",
        "terminal.ansiYellow": "#CB9000",
        "terminal.ansiBrightWhite": "#FFFFFF",
        "terminal.ansiBrightBlack": "#777777",
        "terminal.ansiBrightBlue": "#007ACC",
        "terminal.ansiBrightCyan": "#00AACB",
        "terminal.ansiBrightGreen": "#60CB00",
        "terminal.ansiBrightMagenta": "#E64CE6",
        "terminal.ansiBrightRed": "#F05050",
        "terminal.ansiBrightYellow": "#FFBC5D"
    }
}
